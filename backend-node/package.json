{
  "name": "aurora-backend-node",
  "version": "1.0.0",
  "description": "Node.js implementation of the Aurora music streaming backend",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint --ext .ts src",
    "test": "npm run build",
    "db:migrate": "ts-node src/scripts/migrateLibsql.ts",
    "db:clear-rooms": "ts-node src/scripts/clearRooms.ts"
  },
  "engines": {
    "@fastify/ajv-compiler": "^4.2.0",
    "@fastify/error": "^3.3.0",
    "@types/ws": "^8.5.10",
    "async-mutex": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino-pretty": "^11.2.2",
    "pino": "^9.3.1",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "async-mutex": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.3.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
